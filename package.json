{
  "name": "rider",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "migrate": "dotenv -e .env -- npx prisma migrate dev --schema packages/prisma/schema.prisma",
    "generate": "dotenv -e .env -- npx prisma generate dev --schema packages/prisma/schema.prisma"
  },
  "devDependencies": {
    "dotenv-cli": "^10.0.0",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.5.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^6.15.0"
  }
}
